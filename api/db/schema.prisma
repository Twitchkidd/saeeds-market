datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model TagLine {
  id        Int      @id @default(autoincrement())
  text      String
  createdAt DateTime @default(now())
}

model PrimaryCallToActionText {
  id        Int      @id @default(autoincrement())
  text      String
  createdAt DateTime @default(now())
}

model HeroImage {
  id    Int    @id @default(autoincrement())
  title String
  url   String
}
